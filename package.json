{
  "name": "react-bo",
  "version": "1.0.0",
  "description": "react-bo",
  "main": "src/index.jsx",
  "scripts": {
    "start": "npm run start:client & npm run start:server",
    "start:client": "NODE_ENV=development webpack-serve --config webpack.config.serve.js",
    "start:server": "NODE_ENV=development babel-watch src/server.jsx",
    "clean": "rimraf dist/",
    "build": "npm run clean && npm run build:server & npm run build:client",
    "build:server": "NODE_ENV=production webpack --config webpack.config.server.js",
    "build:client": "NODE_ENV=production webpack --config webpack.config.client.js",
    "lint": "npm run lint:server && npm run lint:client",
    "lint:server": "eslint src/config.js src/server.js src/middlewares/**",
    "lint:client": "eslint src/client.jsx src/store/** src/component/**",
    "deploy:build": "npm run build && docker build -t yuri/react-bo . ",
    "deploy:run": "docker run yuri/react-bo --hostname=yuri -p 49170:5000 -d ",
    "test": "npm run lint && jest",
    "dist": "npm run build && cd dist && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yurikrupnik/react-bo.git"
  },
  "author": "Yuri Krupnik",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yurikrupnik/react-bo/issues"
  },
  "homepage": "https://github.com/yurikrupnik/react-bo#readme",
  "dependencies": {
    "@material-ui/core": "^1.0.0",
    "axios": "^0.18.0",
    "classnames": "^2.2.5",
    "faker": "^4.1.0",
    "flexboxgrid": "^6.3.1",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-compress": "^3.0.0",
    "koa-logger": "^3.2.0",
    "koa-mount": "^3.0.0",
    "koa-render-view": "^1.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.2",
    "koa-views": "^6.1.4",
    "lodash": "^4.17.10",
    "mongodb": "^3.0.6",
    "mongoose": "^5.0.14",
    "normalize.css": "^8.0.0",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-form": "^3.5.5",
    "react-loadable": "^5.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",

{
  "name": "JamesAllen",
  "version": "1.0.0",
  "description": "JamesAllen",
  "main": "src/server.jsx",
  "scripts": {
    "watch:server": "cross-env NODE_ENV=development webpack --config webpack.config.server.js -w",
    "watch:client": "cross-env NODE_ENV=development webpack --config webpack.config.client.js -w",
    "watch": "npm run watch:server & npm run watch:client",
    "start": "concurrently \"npm run start:server\"  \"npm run start:client\"",
    "start:client": "cross-env NODE_ENV=development webpack-serve --config webpack.config.serve.js",
    "start:server": "concurrently \"npm run watch:server\" \"nodemon dist/server.js\"",
    "clean": "rd /s /q dist/",
    "build": "npm run clean && npm run build:server & npm run build:client",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.config.server.js",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack.config.client.js",
    "lint": "eslint eslint --ext js,jsx src",
    "test": "jest",
    "deploy:test": "npm run lint && npm run test",
    "deploy:build": "npm run deploy:test && npm run build && docker build -t yuri/react-bo . ",
    "deploy:run": "docker run yuri/react-bo --hostname=yuri -p 49170:5000 -d ",
    "dist": "npm run build && cd dist && npm start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yurikrupnik/react-bo.git"
  },
  "author": "Yuri Krupnik",
  "license": "ISC",
